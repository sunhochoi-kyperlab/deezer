<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="http://www.obigo.com/ces2020/OBIGO/C300/deezer" version="1.0.10" viewmodes="fullscreen">
    <name xml:lang="en-us" short="Deezer">Deezer</name>
    <icon src="icon.png"/>
    <content src="index.html" type="text/html"/>
    <description xml:lang="en-us">Deezer is music streaming app to play and discover variety of popular music. Enjoy your personal soundtrack and the songs you already love in one infinite stream.</description>
    <author>Deezer</author>
    <license>Obigo</license>
    <thumbnail src="thumbnail.png"/>
    <access origin="*"/>
    <meta-data>
      <manifest-version>1.3.1.1</manifest-version>
    </meta-data>

    <feature name="http://www.obigo.com/api/audio" required="true">
      <param name="version" value="1.1.0.0"/>
    </feature>
    <feature name="http://www.obigo.com/api/storage" required="true">
      <param name="version" value="1.1.0.0"/>
    </feature>
    <feature name="http://www.obigo.com/api/network" required="true">
      <param name="version" value="1.1.0.0"/>
    </feature>
    <feature name="http://www.obigo.com/api/applicationmanager.getownerapplication" required="true"><param name="version" value="3.1.5.7"/></feature>
</widget>
